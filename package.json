  },
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coingaming/moon-ui.git"
  },
  "author": "Yolo Group",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coingaming/moon-ui/issues"
  },
  "homepage": "https://github.com/coingaming/moon-ui#readme",
  "dependencies": {
    "dotenv": "^16.5.0",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4"
  }
}
