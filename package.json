{
  "name": "@heathmont/moon-ui",
  "version": "1.7.0",
  "description": "Moon UI library for generating core and components CSS files",
  "main": "dist/install.js",
  "type": "module",
  "bin": {
    "moon-ui": "dist/install.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "version": "changeset version && npm install && npm update && npm run build && node dist/install.js --add-components",
    "release": "changeset publish"
  },
  "files": [
    "dist/**/*",
    "src/components/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coingaming/moon-ui.git"
  },
  "author": "Yolo Group",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coingaming/moon-ui/issues"
  },
  "homepage": "https://github.com/coingaming/moon-ui#readme",
  "dependencies": {
    "dotenv": "^16.5.0",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/node": "^24.0.8",
    "typescript": "^5.8.3"
  }
}
