{
  "name": "@heathmont/moon-ui",
  "version": "1.4.1",
  "description": "Moon UI library for generating core and components CSS files",
  "main": "src/install.js",
  "type": "module",
  "bin": {
    "moon-ui": "src/install.js"
  },
  "scripts": {
    "changeset": "changeset",
    "version": "changeset version && npm install && node src/install.js --add-components",
