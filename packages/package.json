{
  "name": "@moondesignsystem/ui",
  "version": "3.7.2",
  "private": false,
  "description": "Moon UI library for generating core and components CSS files",
  "main": "dist/install.js",
  "type": "module",
  "bin": "dist/install.js",
  "scripts": {
    "build": "tsc && chmod +x dist/install.js",
    "dev": "tsc --watch",
    "sass:watch": "sass --watch src/styles/components/main.scss dist/styles/moon-components.css --no-source-map",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist/**/*",
    "src/styles/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moondesignsystem/ui.git"
  },
  "author": "Moon Design System",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moondesignsystem/ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://ui.moondesignsystem.com",
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.2",
    "postcss": "^8.5.6",
    "sass": "^1.92.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.2",
    "typescript": "^5.9.2"
  }
}
